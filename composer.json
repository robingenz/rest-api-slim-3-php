{
  "name": "robingenz/rest-api-slim-php",
  "description": "Example of REST API with Slim PHP Framework.",
  "keywords": [
    "slim3",
    "slim",
    "php",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/robingenz/rest-api-slim-php",
  "license": "MIT",
  "authors": [
    {
      "name": "Robin Genz",
      "email": "mail@robingenz.de",
      "homepage": "https://github.com/robingenz"
    }
  ],
  "require": {
    "slim/slim": "3.12",
    "vlucas/phpdotenv": "^3.3",
    "firebase/php-jwt": "^5.0",
    "robmorgan/phinx": "^0.10.6"
  },
  "config": {
    "process-timeout": 0
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
